extends ./../layout

block content
  div#content
    div.photoboxMain
        a(href='gallery/#{photoMain.id}')
          img#indexMain(src='#{photoMain.link}')
    each photo in photos
      div.photobox
        div.thumbContainerImg
          img(src='#{photo.link}')
          a(href='gallery/#{photo.id}')
        div.thumbContainer
          h4 #{photo.title}


